{
  "name": "yoinker",
  "version": "0.1.0",
  "description": "Yoinker is a Discord bot that makes it easy to yoink emotes... or just managing emotes in a Discord server.",
  "author": "osairis100@gmail.com",
  "keywords": [],
  "license": "MIT",
  "main": "bot.js",
  "scripts": {
    "start": "node -r esm -r dotenv/config bot",
    "dev": "nodemon"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "jsonfile": "^6.1.0",
    "sharp": "^0.27.1"
  },
  "devDependencies": {
    "esm": "^3.2.25",
    "nodemon": "^2.0.7"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*"
    ],
    "exec": "npm start",
    "ext": "js"
  }
}
